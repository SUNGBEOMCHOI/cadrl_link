<?xml version="1.0"?>
<launch>
  <node pkg="e2box_imu" type="e2box_imu_node" name="e2box_imu_node" output="screen">
    <param name="port" type="string" value="/dev/ttyUSB2" />
    <param name="baudrate" type="int" value="115200" />
    <param name="yaw_offset" type="double" value="-2.0"/>
  </node>
</launch>
